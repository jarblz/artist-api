<p id="notice"><%= notice %></p>

<h1>Listing Artists</h1>

<table>
  <thead>
    <tr>
      <th>Handle</th>
      <th>Url</th>
      <th>Name</th>
      <th>Bio</th>
      <th>Members</th>
      <th>Website</th>
      <th>Wiki page</th>
      <th>Donation url</th>
      <th>Tags</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.handle %></td>
        <td><%= artist.url %></td>
        <td><%= artist.name %></td>
        <td><%= artist.bio %></td>
        <td><%= artist.members %></td>
        <td><%= artist.website %></td>
        <td><%= artist.wiki_page %></td>
        <td><%= artist.donation_url %></td>
        <td><%= artist.tags %></td>
        <td><%= artist.location %></td>
        <td><%= link_to 'Show', artist %></td>
        <td><%= link_to 'Edit', edit_artist_path(artist) %></td>
        <td><%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist', new_artist_path %>
